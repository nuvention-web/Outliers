	<body>
		<!--###############################-->
	    <!--NAVIGATION ####################-->
	    <!--###############################-->
	    <header id="commonHeader">
			<div class="container">
				<div class="row">
					<!-- Logo -->
					<div id="logo-wrapper-other" class="col-md-4">
						<a class="navbar-brand" href="#">
							<img  alt="Brand" src="../images/logomakeroom.gif">
						</a>
					</div>				
					<!-- Navigation --> 
					<nav class="navbar navbar-default navbar-static-top col-md-8">
						<!-- .navbar-toggle is used as the toggle for collapsed navbar content -->
						
						<!-- Collect the nav links, forms, and other content for toggling -->
						<div id="navbar" class="navbar navbar-right">
					        <form action="/logout" method="post">
						        <button type="submit" class="signout">SIGN OUT</button>
					        </form>
			        	</div>			
					</nav>
				</div>
				<div class="row">
			    	<ul class="nav nav-pills">
					    <nav class="navbar navbar-left">
			    			<ul class="nav navbar-nav">
							  <li><a href="../home" >Home</a></li>
							  <li><a href="../friends" class="active">Friends</a></li>
							  <li><a href="../saved">Loved</a></li>
							  <li><a href="../sent">Sent</a></li>
							</ul>
		    			</nav>
					</ul>
			    </div>
			</div>
		</header>
	    <!--###############################-->
	    <!--TIMELINE   ####################-->
	    <!--###############################-->
	    <div id="timelineHeader" class="row">
	    	<div class="col-sm-5">
	    		<div class="row">
					<img class="img-circle friend" style="float:right" src="../images/<%=user%>.png" alt="Generic placeholder image">
				</div>
				<div class="row"><h3 style="float:right;padding-right:20px;"><%=user%></h3></div>		
			</div>			
			<div class="col-sm-2"><img class="friend" src="http://netdna.webdesignerdepot.com/uploads/ampersand//andalemono.jpg"></div>
			<div class="col-sm-5">
				<div class="row">
					<img class="img-circle friend" style="float:left;" src="../images/<%=friend%>.png" alt="Generic placeholder image">
				</div>
				<div class="row"><h3 style="float:left; padding-left:20px"><%=friend%></h3></div>
    			</div>    			
			</div>
	    </div>
	    <section id="cd-timeline" class="cd-container">	
	
	<% for(var i=0; i< message.length; i++) { %> 					
	
			<div class="cd-timeline-block">
				<div class="cd-timeline-img cd-movie">
					<img src="/images/link.png" alt="link">
				</div> <!-- cd-timeline-img -->

				<div class="cd-timeline-content">
					<a class="msgTitle" href=' <%= url[i] %>'><%=title[i]%></a>
					<a class="embedly-card" href=' <%= url[i] %>'></a>
					<div class="msg"><span><%= message[i] %></span></div>
					<span class="cd-date"><%= time[i] %></span>
	    			<!--div class="msg"><span>Do you wanna catch this movie this weekend?</span></div-->
				</div> <!-- cd-timeline-content -->
				
			</div> <!-- cd-timeline-block -->

<% }%>
			

		</section> <!-- cd-timeline -->
